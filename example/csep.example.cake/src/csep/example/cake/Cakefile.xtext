grammar csep.example.cake.Cakefile with csep.CoffeeScript

generate cakefile "http://www.example.csep/cake/Cakefile"

Cakefile:
	tasks+=TaskDeclaration*;
	
TaskDeclaration:
	TASK CALL_START name=StringLiteral COMMA description=StringLiteral COMMA (options=Id)? FUNC_ARROW
		body=Block CALL_END TERMINATOR;

terminal TASK: 'task';